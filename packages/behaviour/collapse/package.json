{
  "name": "@lit-element-bootstrap/collapse",
  "version": "1.0.0-alpha.2",
  "description": "Bootstrap v4.1.3 collapse behaviour elements in LitElement",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "license": "MIT",
  "author": "Nikolas Christou",
  "homepage": "https://github.com/nik-christou/lit-element-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/nik-christou/lit-element-bootstrap"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "global.d.ts",
    "bs-*.js",
    "bs-*.d.ts",
    "css/-*.js",
    "css/*.d.ts",
    "custom-elements.json"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "npm run wca-analyze:json",
    "lit-analyze": "lit-analyzer bs-*.js css/*.js --failFast",
    "wca-analyze:json": "wca analyze . --outFile custom-elements.json",
    "lint:prettier": "prettier \"./**/*.js\" --tab-width 4 --list-different",
    "format:prettier": "prettier \"./**/*.js\" --tab-width 4 --write"
  },
  "dependencies": {
    "@lit-element-bootstrap/content": "^1.0.0-alpha.3",
    "lit-element": "^2.2.1",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.3.4",
    "@open-wc/karma-esm": "^2.7.1",
    "@open-wc/testing-karma": "^3.1.38",
    "deepmerge": "^4.1.1",
    "karma": "^4.3.0",
    "sinon": "^7.5.0",
    "lit-analyzer": "^1.1.8",
    "@open-wc/prettier-config": "^0.1.12",
    "eslint": "^6.6.0",
    "web-component-analyzer": "^0.1.21"
  }
}
